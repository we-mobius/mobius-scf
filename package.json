    "cz-customizable": "^6.2.1",
    "eslint": "^7.3.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "inquirer": "^7.3.0",
    "lint-staged": "^10.2.11",
    "mincloud": "^1.0.6",
    "shelljs": "^0.8.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "yargs": "^15.4.0"
  },
